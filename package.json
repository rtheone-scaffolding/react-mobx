{
  "name": "scaffold-react-mobx",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Jonathan Chuang",
  "license": "MIT",
  "scripts": {
    "bundle": "./node_modules/.bin/webpack",
    "bundle-prod": "./node_modules/.bin/webpack --config webpack.prod.config.js",
    "test": "./node_modules/.bin/jest",
    "prettier": "./node_modules/.bin/prettier --config .prettierrc.json --write src/**/*.{ts,tsx}",
    "start": "./node_modules/.bin/webpack-dev-server"
  },
  "dependencies": {
    "core-js": "^2.5.7",
    "fela": "^6.2.1",
    "mobx": "4.4.0",
    "mobx-react": "^5.2.6",
    "mobx-utils": "^5.0.2",
    "normalize.css": "^8.0.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-fela": "^8.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@types/jest": "^23.3.1",
    "babel-loader": "^8.0.2",
    "babel-plugin-css-to-js": "^0.0.2",
    "css-loader": "^1.0.0",
    "jest": "^23.5.0",
    "mini-css-extract-plugin": "^0.4.2",
    "optimize-css-assets-webpack-plugin": "^4.0.3",
    "prettier": "^1.14.2",
    "prop-types": "^15.6.2",
    "ts-jest": "^23.1.4",
    "typescript": "^3.0.3",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-closure-compiler": "^2.1.6",
    "webpack-dev-server": "^3.1.7"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
